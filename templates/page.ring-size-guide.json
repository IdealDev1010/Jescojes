    "image_with_text_KPCXAn": {
      "type": "image-with-text",
      "blocks": {
        "heading_QhQpNj": {
          "type": "heading",
          "settings": {
            "heading": "Mastering Ring Sizing: Your Essential Guide to Finding the Perfect Fit",
            "heading_size": "h1",
            "heading_tag": "h2"
          }
        },
        "text_darBBM": {
          "type": "text",
          "settings": {
            "text": "<ol><li><strong>Prepare Your Materials:<\/strong> Gather a flexible measuring tape or a piece of string, a ruler, a pen or marker, and a plain piece of paper.<\/li><li><strong>Wrap the String Around Your Finger:<\/strong> Take the string or measuring tape and wrap it around the base of the finger you want to measure. Make sure it's snug but not too tight.<\/li><li><strong>Mark the String:<\/strong> Use the pen or marker to mark where the string overlaps or meets itself.<\/li><li><strong>Measure the Length:<\/strong> Carefully remove the string from your finger and lay it flat on a piece of paper. Use a ruler to measure the length from the end to the marked point in millimeters.<\/li><li><strong>Find Your Size:<\/strong> Compare the measured length to a ring size chart, either provided by the jeweler or available online, to determine your ring size. Be sure to check the sizing chart against the sizing method used (e.g., US, UK, EU) to ensure accuracy.<\/li><\/ol>",
            "text_size": "typeset2",
            "secondary_color": false
          }
        }
      },
      "block_order": [
        "heading_QhQpNj",
        "text_darBBM"
